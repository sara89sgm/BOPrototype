extends ../layouts/themeDefault

block content
	.container.loaded
		div#fb-root
			script
			(function(d, s, id) {
			   var js, fjs = d.getElementsByTagName(s)[0];
			   if (d.getElementById(id)) {return;}
			   js = d.createElement(s); js.id = id;
			   js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId=101562769948573";
			   fjs.parentNode.insertBefore(js, fjs);
			}(document, 'script', 'facebook-jssdk'));

			div.fb-like(data-href="http://foodjing.com/" data-send="true" data-width="450" data-show-faces="true")
			
		#skrollr-body
			main
				section#home.homeSlide
					.bcg(data-center="background-position: 50% 0px;" data-top-bottom="background-position: 50% -100px;" data-anchor-target="#home")
            			.hsContainer
            				.hsContent(data-center="opacity: 1" data-50-top="opacity: 0" data-anchor-target="#home .social") 
            					.logo
            						img (src="img/logo.png")

            	 section#home-after.homeSlide
                    .bcg (data-center="background-position: 50% 0px;" data-top-bottom="background-position: 50% 0px;" data-anchor-target="#home")
                        .hsContainer
                        	.hsContent
                          
block js
	script(src='js/vendor/jquery-2.1.0.js')
	script(src='js/vendor/owl.carousel.js')
	script(src='js/vendor/skrollr.js')
	script(src='js/vendor/skrollr.menu.js')
	script(src='js/vendor/waypoints.min.js')
	script(src='js/init.js')

    
 
		